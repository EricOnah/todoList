{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node",
        "config": {
          "memory": 1024,
          "maxDuration": 60 
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.js"
      }
    ],
    "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
  }